{
  "name": "babel-react-i18n-helper",
  "version": "1.1.1",
  "description": "A babel plugin for helping i18n",
  "main": "lib/index.js",
  "scripts": {
    "build": "rm -rf lib && babel src/index.js -d lib",
    "test": "babel test/index.js -d test/compiled --watch",
    "patch": "npm --no-git-tag-version version patch",
    "minor": "npm --no-git-tag-version version minor"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/misakisaysyes/babel-react-i18n-helper.git"
  },
  "keywords": [
    "babel-plugin",
    "i18n"
  ],
  "author": "misakisaysyes",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "eslint": "^7.30.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0"
  }
}
